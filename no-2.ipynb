        "#NOMOR 2\n",
        "\n",
        "def BubbleSort(blist):\n",
        "  for passnum in range(len(blist)-1,0,-1):\n",
        "    for i in range(passnum):\n",
        "      if blist[i]>blist[i+1]:\n",
        "        temp = blist[i]\n",
        "        blist[i] = blist[i+1]\n",
        "        blist[i+1] = temp\n",
        "\n",
        "blist = [5,2,7]\n",
        "BubbleSort(blist)\n",
        "print(blist)"
      ]
